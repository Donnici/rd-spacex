{
	"name": "@rd-spacex/webservice",
	"version": "0.0.0",
	"description": "webservice for rd-spacex application",
	"author": "Bruno Dias Donnici <bruno.dias.donnici@gmail.com>",
	"homepage": "",
	"main": "dist/index.js",
	"scripts": {
		"start:api": "node ./dist/src/index.js",
		"dev:api:debug": "ts-node-dev --inspect=0.0.0.0:7001 --debug --exit-child --trace-warnings --ignore-watch node_modules --project ./tsconfig.json -r tsconfig-paths/register -r dotenv/config ./src/index.ts",
		"typecheck": "tsc -p ./tsconfig.json --noEmit",
		"eslint:fix": "eslint './**/*.{ts,tsx}' --config .eslintrc.json --fix",
		"prettier:format": "prettier --ignore **/node_modules/** --write './**/*.{ts,tsx}'",
		"lint": "yarn prettier:format && yarn eslint:fix && yarn typecheck",
		"type": "tsc --sourceMap false -p ./tsconfig.json && tscpaths -p ./tsconfig.json -s . -o ./dist",
		"build": "rm -rf ./dist && yarn lint && yarn type",
		"test": "jest --no-cache --u --no-watchman --detectOpenHandles --forceExit"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Donnici/rd-spacex"
	},
	"keywords": [
		"api",
		"rest"
	],
	"_moduleAliases": {
		"#root": "./dist",
		"#exceptions": "./dist/src/architecture/exceptions",
		"#interfaces": "./dist/src/shared/interfaces",
		"#architecture": "./dist/src/architecture",
		"#domain": "./dist/src/domain",
		"#providers": "./dist/src/providers",
		"#shared": "./dist/src/shared"
	},
	"dependencies": {
		"axios": "^0.24.0",
		"cors": "^2.8.5",
		"dotenv": "^10.0.0",
		"express": "^4.17.1",
		"module-alias": "^2.2.2",
		"pino": "^7.1.0",
		"pino-http": "^6.0.0",
		"pino-pretty": "^7.2.0",
		"rambda": "^6.9.0",
		"source-map-support": "^0.5.20",
		"typescript": "^4.4.4"
	},
	"devDependencies": {
		"@types/axios": "^0.14.0",
		"@types/cors": "^2.8.12",
		"@types/debug": "^4.1.7",
		"@types/dotenv": "^8.2.0",
		"@types/express": "^4.17.13",
		"@types/faker": "^5.5.9",
		"@types/jest": "^27.0.2",
		"@types/node": "^16.11.6",
		"@types/pino-http": "^5.8.0",
		"@types/supertest": "^2.0.11",
		"@typescript-eslint/eslint-plugin": "^5.3.0",
		"@typescript-eslint/parser": "^5.3.0",
		"cz-conventional-changelog": "^3.3.0",
		"debug": "^4.3.2",
		"eslint": "^8.1.0",
		"eslint-config-airbnb-base": "^14.2.1",
		"eslint-config-prettier": "^8.3.0",
		"eslint-import-resolver-typescript": "^2.5.0",
		"eslint-plugin-import": "^2.25.2",
		"eslint-plugin-import-helpers": "^1.1.0",
		"eslint-plugin-jest": "^25.2.2",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-security": "^1.4.0",
		"faker": "^5.5.3",
		"jest": "^27.3.1",
		"jest-module-name-mapper": "^0.1.5",
		"pino": "^7.1.0",
		"prettier": "^2.4.1",
		"rambda": "^6.9.0",
		"supertest": "^6.1.6",
		"ts-node-dev": "^1.1.8",
		"tscpaths": "^0.0.9",
		"typescript": "^4.4.4"
	}
}
